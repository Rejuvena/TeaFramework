<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\..\tModLoader.targets" />
    <PropertyGroup>
        <AssemblyName>TeaFramework</AssemblyName>
        <TargetFramework>net6.0</TargetFramework>
        <PlatformTarget>AnyCPU</PlatformTarget>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <Title>Tomat.Rejuvena.TeaFramework</Title>
        <Authors>Rejuvena Team</Authors>
        <Description>Progressive tModLoader library mod.</Description>
        <PackageProjectUrl>https://github.com/rejuvena/tea-framework</PackageProjectUrl>
        <RepositoryUrl>https://github.com/rejuvena/tea-framework</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
        <PackageReference Include="tModLoader.CodeAssist" Version="0.1.*" />
    </ItemGroup>
    <ItemGroup>
        <Reference Include="Tomlet, Version=3.1.3.0, Culture=neutral, PublicKeyToken=null">
            <HintPath>lib\Tomlet.dll</HintPath>
        </Reference>
    </ItemGroup>
</Project>